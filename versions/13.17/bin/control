#!/bin/bash -e

PATH=/bin/:/usr/bin:$PATH

source $OPENSHIFT_CARTRIDGE_SDK_BASH

client_result "Controll Main called 13.17"

function start { 
    client_result "start"

}

function catchall {
    client_result "   $1: not yet implemented"
}

case "$1" in
  start)       start ;;
  stop)        catchall ;;
  restart)     catchall ;;
  status)      catchall ;;
  deploy)      catchall ;;
  reload)      catchall ;;
  tidy)        catchall ;;
  pre-build)   catchall ;;
  build)       catchall ;;
  post-deploy) catchall ;;
  pre-repo-archive) catchall ;;
  update-configuration) catchall ;;
  *)           exit 0
esac

exit 0
